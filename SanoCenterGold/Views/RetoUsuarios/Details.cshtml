@model SanoCenterGold.Models.RetoUsuario

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>RetoUsuario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Reto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reto.Nombre)
        </dd>
        <dt class = "col-sm-2">
            Entrenador
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reto.Entrenador.Nombre)
        </dd>
        <dt class = "col-sm-2">
            Fecha Finalización
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCompletado)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdRetoUsuario">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
